  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc --sourceMap --noImplicitAny -m commonjs --target ES5 firefox/**/*.ts firefox/**/**/*.ts",
    "xpi": "cd ./firefox; jpm xpi; cd -",
    "run": "cd ./firefox; jpm run -b $(which firefox) --prefs ../prefs.json --debug; cd -",
    "build": "browserify firefox/data/metrics-integration/main.ts -p [ tsify --noImplicitAny --sourceMap --target ES5 ] --debug > firefox/data/metrics-integration/twitter-assistant-content.js",
    "watch": "watchify firefox/data/metrics-integration/main.ts -p [ tsify --noImplicitAny --sourceMap --target ES5 ] --debug -o firefox/data/metrics-integration/twitter-assistant-content.js -v --ignore WNdb --ignore lapack"
  },
  "devDependencies": {
    "watchify": "^3.3.1"
  },
  "main": "index.js",
  "id": "jid1-BptUkbczEocdyg",
  "description": "Twitter Assistant ==============",
  "dependencies": {
    "browserify": "^11.0.1",
    "jpm": "^1.0.1",
    "natural": "^0.2.1",
    "tsify": "^0.11.9",
    "typescript": "^1.5.3"
  }
}
